#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
char s1[100];
    scanf("%[^\n]s",s1);
    for(int i=0;i<strlen(s1);i++){
        if(s1[i]>=97&&s1[i]<=122){
            s1[i]=s1[i]-32;
        }
    }
    printf("%s",s1);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
