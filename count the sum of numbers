import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String st=s.nextLine();
        int sum=0;
        for(int i=0;i<st.length();i++){
            if(st.charAt(i)>=48&&st.charAt(i)<=57){
                int c=st.charAt(i);
                sum+=st.charAt(i)-48;
            }
        }
        System.out.print(sum);/* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
