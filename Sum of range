import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int l=s.nextInt();
        int u=s.nextInt();
        int sum=0;
        for(int i=l;i<=u;i++){
            sum+=i;
        }
        System.out.print(sum);/* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
