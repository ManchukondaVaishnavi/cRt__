#include <stdio.h>
#include<math.h>
int main() {
   int a,b;
   scanf("%d%d",&a,&b);
   int c;
   scanf("%d",&c);
   switch(c){
       case 1:
       printf("%d",a+b);
       break;
       case 2:
       printf("%d",a-b);
       break;
       case 3:
       printf("%d",a*b);
       break;
       case 4:
       printf("%d",a/b);
       break;
   }
}
