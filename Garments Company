#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }int c=0;
    for(int i=0;i<n;i++){
        int k=(int)sqrt(a[i]);
        if(k*k==a[i]){
           c++; 
        }
    }
    printf("%d",c);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
