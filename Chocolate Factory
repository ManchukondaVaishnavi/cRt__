#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n;
    scanf("%d",&n);
    int a[n],b[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }int k=0;
    for(int i=0;i<n;i++){
        if(a[i]!=0){
            b[k++]=a[i];
        }
    }
    for(int i=k;i<n;i++){
        b[i]=0;
    }
    for(int i=0;i<n;i++){
        printf("%d ",b[i]);
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
