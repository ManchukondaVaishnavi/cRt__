// Online C compiler to run C program online
#include <stdio.h>
#include<string.h>

int main() {
  char s[100],s1[100];
  scanf("%s",s);
  scanf("%s",s1);
for(int i=0;s[i]!='\0'||s1[i]!='\0';i++){
    if(s[i]!='\0'){
    printf("%c",s[i]);
    }
    if(s1[i]!='\0'){
    printf("%c",s1[i]);
}
}
return 0;
}
