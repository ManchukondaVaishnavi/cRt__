#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n;
    scanf("%d",&n);
    if(n<=100||n>=200){
        printf("INVALID INPUT");
    }
    else{
        int r=n/4;
        printf("%d",r);
         printf("\n%d",r);
         printf("\n%d",r);
        printf("\n%d",r+(n%4));
        
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
