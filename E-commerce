#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
char s[100];
    scanf("%s",s);
    int max=0,min=122;
    for(int i=0;i<strlen(s);i++){
        if(max<s[i]){
            max=s[i];
        }
        if(min>s[i]){
            min=s[i];
        }
    }
    printf("%d",(max+min));
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
