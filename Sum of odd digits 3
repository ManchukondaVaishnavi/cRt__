import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int sum=0;
        while(n>0){
           int x=n%10;
            if(x%2!=0){
            sum+=x;
            }
            n=n/10;
        }
        System.out.print(sum);/* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
