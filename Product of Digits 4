        while(n>1){
            int x=n%10;
            if(x==0){
            }
            else{
            p*=x;
            }
            n=n/10;
        }
        System.out.print(p);/* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
