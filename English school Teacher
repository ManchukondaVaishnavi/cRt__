import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String st=s.nextLine();
        int c=0,max=0;
        for(int i=0;i<st.length();i++){
            c++;
            if(st.charAt(i)==' '){
                if(max<c){
                    max=c;
                }
                c=0;
            }
        }
        System.out.print(max-1);/* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
