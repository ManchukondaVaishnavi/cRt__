import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int t=n;
        int i=1;
        while(n>0){
            n=n/10;
            i++;
        }
        System.out.println(t%(i-1));/* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
