#include <stdio.h>
#include<math.h>
int main() {
   float a,b,c;
    scanf("%f%f%f",&a,&b,&c);
    float d=b*b-4*a*c;
    if(d<0){
        printf("Roots are imaginary");
    }
    else{
    float r1=(float)(-b+sqrt(d))/2*a;
    float r2=(float)(b+sqrt(d))/2*a;
    printf("%f and %f",r1,r2);
    }
}
