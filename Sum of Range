#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int min,max;
    scanf("%d%d",&min,&max);
    int sum=0;
    int i=min;
    while(i<=max){
        sum+=i;
        i++;
    }
    printf("%d",sum);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
