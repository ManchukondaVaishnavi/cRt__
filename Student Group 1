import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        if(n>=100&&n<=200){
        int t=n/4;
        System.out.print(t+"\n"+t+"\n"+t+"\n"+(t+(n%4)));
        }
        else{
            System.out.print("INVALID INPUT");/* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
    }
}
