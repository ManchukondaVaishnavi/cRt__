#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
char s1[100],s2[100];
    scanf("%s",s1);
    int k=0;
    for(int i=strlen(s1)-1;i>=0;i--){
        s2[k++]=s1[i];
    }
    s2[k]='\0';
    int x=strcmp(s1,s2);
    if(x==0){
        printf("Palindrome");
    }
    else{
        printf("Not a Palindrome");
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
