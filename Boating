#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
       int w,a,c;
    scanf("%d%d%d",&w,&a,&c);
    if((a*75+c*30)<=w){
        printf("Boat is stable");
    }
    else{
        printf("Boat will drown");
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
