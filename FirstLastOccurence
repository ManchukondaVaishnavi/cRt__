import java.util.*;
public class FirstLastOccurence{
public static void main(String [] args){
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int x=s.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++){
a[i]=s.nextInt();
}
Arrays.sort(a);
for(int  i=0;i<n;i++){
if(a[i]==x){
System.out.print(i+" ");
break;
}
else if(i==n-1){
System.out.print("-1 ");
}
}
for(int j=n-1;j>=0;j--){
if(a[j]==x){
System.out.print(j);
break;
}
else if(j==0){
System.out.print("-1");
}
}
}
}
