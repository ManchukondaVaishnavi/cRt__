#include <stdlib.h>

int main() {
int n=0;
    char s[100];
    scanf("%[^\n]s",s);
    for(int i=0;i<strlen(s);i++){
        if((s[i]>=65&&s[i]<=90)||(s[i]>=97&&s[i]<=122)){
            
        }
        else if((s[i]>=48&&s[i]<=57)||s[i]==32){
           
        }
        else{
            n++;
        }
        
    }
    printf("%d",n);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
