#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int x1,y1,x2,y2,x3,y3;
    scanf("%d%d%d%d%d%d",&x1,&y1,&x2,&y2,&x3,&y3);
    float x=(float)(x1+x2+x3)/3;
    float y=(float)(y1+y2+y3)/3;
    printf("%.1f",x);
    printf("\n%.1f",y);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
