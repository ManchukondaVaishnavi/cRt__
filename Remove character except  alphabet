#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
char s[200];
    scanf("%[^\n]s",s);
    for(int i=0;i<strlen(s);i++){
        if((s[i]>=65&&s[i]<=90)||(s[i]<=122&&s[i]>=97)||s[i]==32){
            printf("%c",s[i]);
        }
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
