int main() {
int n;
    scanf("%d",&n);
    int i=1;
    int r1=7,r2=5;
    int x=n;
    n=n/2;
    while(i<=n){
        i++;
        printf("%d ",r1);
        r1++;
        printf("%d ",r2);
        r2++;
        
    }
   if(x%2!=0){
       printf("%d",r1);
   }
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
